{
  "name": "user-stories-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "ts-node src/seeds/seed.ts",
    "docker:build": "docker build -t sportsline-api .",
    "docker:run": "docker run -d --name sportsline-api -p 3000:3000 --env-file .env sportsline-api",
    "docker:stop": "docker stop sportsline-api",
    "docker:remove": "docker rm sportsline-api",
    "docker:logs": "docker logs -f sportsline-api",
    "docker:compose:up": "docker-compose up --build",
    "docker:compose:up:d": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "docker:compose:prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:compose:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:compose:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kale-8/user-stories-node.git"
  },
